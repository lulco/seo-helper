{
	"name": "lulco/seo-helper",
	"description": "Library for more simple work with meta data",
	"keywords": ["PHP", "Meta data", "Page title", "Open Graph tags", "Twitter cards"],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Michal Lulco",
			"email": "michal.lulco@gmail.com"
		}
	],
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"nette/application": "^3.0",
		"phpunit/phpunit": "^9.5 | ^10.0 | ^11.5 | ^12.5"
	},
	"suggest": {
		"nette/application": "Allows to use SeoHelper\\Bridge\\Nette\\Components\\MetaData\\MetaDataControl"
	},
	"autoload": {
		"psr-4": {
			"SeoHelper\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SeoHelper\\Tests\\": "tests/"
		}
	}
}
